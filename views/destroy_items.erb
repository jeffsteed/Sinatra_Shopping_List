<h1>Remove Item</h1>
<input type="button" onclick="window.location.href='/logout'" value="Logout"><br>
<p>
  Are you sure you want to remove <%= @item.name %>?
</p>

<form action="/destroy" method="post">
  <input type="hidden" name="id" value="<%= @item.id %>">
  <input type="submit" value="Remove">
  <input type="button" onclick="window.location.href='/items'" value="Cancel">
</form>
